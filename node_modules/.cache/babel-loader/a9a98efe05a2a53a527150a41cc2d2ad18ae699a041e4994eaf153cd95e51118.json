  :dialog=\"dialog\"\n    :dialogKey=\"add\"\n    :buttonLoader=\"buttonLoader\"\n    @close-dialog=\"dialog = false\"\n    @add-new-account=\"addNewAccount\"\n  />\n  <div class=\"mt-13 text-right mr-5\">\n    <v-btn\n      variant=\"text\"\n      color=\"deep-orange\"\n      class=\"text-capitalize\"\n      @click=\"dialog = true\"\n    >\n      <v-icon icon=\"mdi-plus\"></v-icon>\n      add new account\n    </v-btn>\n  </div>\n  <div\n    v-if=\"loader\"\n    class=\"d-flex justify-center align-center\"\n    style=\"height:65vh\">\n    <v-progress-circular\n      indeterminate\n      size=\"50\"\n      color=\"deep-orange\"\n    ></v-progress-circular>\n  </div>\n  <div v-else>\n    <div\n      v-if=\"!cards.length\"\n      class=\"txt32 font-weight-medium\"\n      style=\"height:60vh\">\n      <div class=\"not-found \">\n        Data not exists \n      </div>\n    </div>\n    <div v-else>\n      <div v-for=\"(item, index) in cards\" :key=\"index\" class=\"d-flex\">\n        <Card\n          :card=\"item\"\n          @delete-card=\"deleteAccount\"\n          @update-card=\"updateAccount\"\n        />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Card from \"@/components/cards/index-card.vue\";\nimport Dialog from \"@/components/dialog/index-dialog.vue\";\nimport { ref } from \"vue\";\nimport axios from \"axios\";\nimport { useRoute } from \"vue-router\";\nimport { useRouter } from \"vue-router\";\nimport useToast from \"@/plugins/useToast.js\";\nimport { onMounted } from \"vue\";\nexport default {\n  components: {\n    Card,\n    Dialog\n  },\n  setup() {\n    const cards = ref([])\n    const loader = ref(true);\n    const route = useRoute();\n    const router = useRouter();\n    const dialog = ref(false);\n    const add = ref(\"add\");\n    const buttonLoader = ref(false)\n\n    const getAllAccounts = () => {\n      const token = JSON.parse(localStorage.getItem('token'))\n      const appBaseURL = process.env.VUE_APP_URL\n\n        let config = {\n          method: 'get',\n          maxBodyLength: Infinity,\n          url: `${appBaseURL}get_all_passwords`,\n          headers: { \n            'Content-Type': 'application/json', \n            'Authorization': `Bearer ${token}`,\n          },\n        };\n\n        axios.request(config)\n        .then((response) => {\n          console.log(JSON.stringify(response));\n          cards.value = response?.data?.passwords\n          if(response?.data?.message == 'Passwords successfully get')\n          {\n            console.log(response?.data?.message);\n          }\n          else {\n            console.log(response);\n            useToast(response?.data?.message, \"error\");\n          }\n        })\n        .catch((error) => {\n          console.log(error);\n          useToast(error?.message ? error.message : \"Something went wrong\", \"error\");\n        })\n        .finally(() => {\n          loader.value = false\n      });\n    };\n\n    function deleteAccount(object) {\n      console.log(\"deleted object ===\", object);\n      getAllAccounts();\n      useToast(\"Account deleted successfully\", \"success\");\n    }\n\n    const addNewAccount = (payload) => {\n      console.log('get update payload >>>', payload.cardObj)\n      let data = JSON.stringify(payload.cardObj);\n      const token = JSON.parse(localStorage.getItem('token'))\n      const appBaseURL = process.env.VUE_APP_URL\n\n        let config = {\n          method: 'post',\n          maxBodyLength: Infinity,\n          url: `${appBaseURL}create_password`,\n          headers: { \n            'Content-Type': 'application/json', \n            'Authorization': `Bearer ${token}`,\n          },\n          data : data\n        };\n\n        axios.request(config)\n        .then((response) => {\n          console.log(JSON.stringify(response));\n          if(response.data.message == 'Password entry created successfully')\n          {\n            getAllAccounts();\n            useToast(response.data.message, \"success\");\n          }\n          else {\n            console.log(response);\n            useToast(response?.data?.message ? response?.data?.message : \"New account did not add\", \"error\");\n          }\n        })\n        .catch((err) => {\n          console.log(err);\n          useToast(err?.message ? err?.message : \"New account did not add\", \"error\");\n        })\n        .finally(() => {\n          dialog.value = payload.key;\n          buttonLoader.value = false\n      });\n    };\n\n    function updateAccount(object) {\n      console.log(\"updated object ====\", object);\n      getAllAccounts();\n      useToast(\"Account updated successfully\", \"success\");  \n    }\n\n    onMounted(() => {\n      setTimeout(() => {\n        loader.value = false\n      }, 1000);\n      getAllAccounts();\n    });\n\n    return {\n      loader,\n      buttonLoader,\n      cards,\n      add,\n      dialog,\n      route,\n      router,\n      getAllAccounts,\n      deleteAccount,\n      addNewAccount,\n      updateAccount\n    };\n  },\n\n  methods: {},\n};\n</script>\n<style scoped>\n.not-found {\n  position: absolute;\n  top: 50%; right: 50%;\n  transform: translate(50%,-50%)\n}\n</style>"],"mappings":";;;EAQOA,KAAK,EAAC;AAAuB;;;EAahCA,KAAK,EAAC,oCAAoC;EAC1CC,KAAmB,EAAnB;IAAA;EAAA;;;;;;;EAUED,KAAK,EAAC,0BAA0B;EAChCC,KAAmB,EAAnB;IAAA;EAAA;;gEACAC,mBAAA,CAEM;EAFDF,KAAK,EAAC;AAAY,GAAC,mBAExB;oBAFAG,UAEM,C;;;;;;;;;;6DAnCVC,YAAA,CAMEC,iBAAA;IALCC,MAAM,EAAEC,MAAA,CAAAD,MAAM;IACdE,SAAS,EAAED,MAAA,CAAAE,GAAG;IACdC,YAAY,EAAEH,MAAA,CAAAG,YAAY;IAC1BC,aAAY,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEN,MAAA,CAAAD,MAAM;IACpBQ,eAAe,EAAEP,MAAA,CAAAQ;uFAEpBb,mBAAA,CAUM,OAVNc,UAUM,GATJZ,YAAA,CAQQa,gBAAA;IAPNC,OAAO,EAAC,MAAM;IACdC,KAAK,EAAC,aAAa;IACnBnB,KAAK,EAAC,iBAAiB;IACtBoB,OAAK,EAAAR,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEN,MAAA,CAAAD,MAAM;;sBAEd,MAAiC,CAAjCF,YAAA,CAAiCiB,iBAAA;MAAzBC,IAAI,EAAC;IAAU,I,iBAAU,mBAEnC,E;;QAGMf,MAAA,CAAAgB,MAAM,I,cADdC,mBAAA,CASM,OATNC,UASM,GALJrB,YAAA,CAIuBsB,8